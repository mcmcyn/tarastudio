<% include ../partials/header %>
<div class="container">
	<h1 class="text-center">List a New Studio</h1>
		<form class="form-styling" action="/studios" method="POST">
			<div class="form-group">
				<input class="form-control" type="text" name="name" placeholder="studio name">
			</div>
			<div class="form-group">
				<input class="form-control" type="text" name="image" placeholder="studio image">
			</div>
			<div class="form-group">
				<input class="form-control" type="text" name="description" placeholder="description">
			</div>
			<div class="form-group">
				<button class="btn btn-primary btn-lg btn-block">Add Studio</button>
			</div>
			<a href="/studios">Go back</a>
		</form>
</div>
<% include ../partials/footer %>